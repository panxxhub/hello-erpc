{
	"editor.fontFamily": "'Monaspace Argon', 'Jetbrains Mono', 'Fira Code Retina', 'Monaspace Argon Var', 'Monaspace Radon Var', 'Monaspace Xenon', 'Monaspace Krypton', 'Cascadia Code', Consolas, 'Courier New', monospace",
	"editor.fontSize": 17,
	"editor.fontLigatures": "'calt','liga','dlig','ss01','ss02','ss03','ss04','ss05','ss06','ss07','ss08'",
	"editor.inlayHints.enabled": "on",
	"editor.inlayHints.fontFamily": "'Monaspace Radon Var', 'Monaspace Argon', 'Monaspace Xenon', 'Monaspace Krypton', 'Fira Code Retina', 'Jetbrains Mono', 'Cascadia Code', Consolas, 'Courier New', monospace",
	"editor.inlineSuggest.enabled": true,
	"editor.fontVariations": true,
	"editor.linkedEditing": true,
	"editor.formatOnSave": true,
	"editor.stickyScroll.enabled": true,
	"github.copilot.advanced": {
		"enabled": true,
		"showAdvancedSuggestions": true,
		"showBasicSuggestions": true,
		"showComments": true,
		"showLiterals": true,
		"showMethods": true,
		"showSnippets": true,
		"showVariables": true,
		"showWords": true,
	},
	"github.copilot.enable": {
		"*": true,
		"plaintext": false,
		"scminput": false
	},
	"clangd.checkUpdates": false,
	"clangd.serverCompletionRanking": true,
	"clangd.path": "/usr/bin/clangd-18",
	"clangd.onConfigChanged": "restart",
	"clangd.arguments": [
		"--compile-commands-dir=${workspaceFolder}/build",
		"--pch-storage=memory",
		"--clang-tidy=true",
		"--all-scopes-completion=true",
		"--background-index=true",
		"--inlay-hints=true",
		"--pretty",
	],
	"C_Cpp.default.intelliSenseMode": "gcc-arm",
	"C_Cpp.intelliSenseEngine": "default",
	"C_Cpp.intelliSenseEngineFallback": "disabled",
	"C_Cpp.clang_format_path": "clang-format-18",
	"C_Cpp.clang_format_fallbackStyle": "Visual Studio",
	"C_Cpp.autocomplete": "default",
	"C_Cpp.codeFolding": "enabled",
	"C_Cpp.enhancedColorization": "enabled",
	"C_Cpp.autocompleteAddParentheses": true,
	"C_Cpp.loggingLevel": "Information",
	"C_Cpp.autoAddFileAssociations": true,
	"C_Cpp.codeAnalysis.clangTidy.enabled": false,
	"C_Cpp.codeAnalysis.clangTidy.path": "/usr/bin/clang-tidy-18",
	"C_Cpp.codeAnalysis.runAutomatically": true,
	"editor.semanticHighlighting.enabled": true,
	"editor.renderLineHighlight": "all",
	"editor.renderWhitespace": "boundary",
	"editor.minimap.renderCharacters": true,
	"terminal.integrated.fontFamily": "'Hasklug NF', 'Monaspace Argon Var'",
	"workbench.colorTheme": "Default Dark Modern",
	"editor.semanticTokenColorCustomizations": {
		"rules": {
			"enum": {
				"fontStyle": "bold"
			},
			"enumMember": {
				"foreground": "#a0cd00",
				"fontStyle": "italic bold",
				"italic": true
			},
			"constant": {
				"fontStyle": "bold underline",
				"foreground": "#b9d1cc"
			},
			"constParameter": {
				"fontStyle": "bold underline",
				"foreground": "#b9d1cc"
			},
			"macro": {
				"foreground": "#2cac61",
				"fontStyle": "bold underline"
			},
			"function": {
				"fontStyle": "bold"
			},
			"variable": {
				"fontStyle": "italic",
			},
			"parameter": {
				"foreground": "#00b0ea",
				"fontStyle": "italic bold"
			},
			"magicFunction": {
				"foreground": "#ff0000"
			},
			"property": {
				"fontStyle": "italic underline",
				"foreground": "#c943e1"
			}
		}
	},
	"cSpell.words": [
		"ADMA",
		"ALCONTROL",
		"ALERR",
		"ALEVENTMASK",
		"ALLBUTINITMASK",
		"ALSTATUS",
		"BITARR",
		"BITSPOS",
		"BYTESOFFSET",
		"delayable",
		"DLSTATUS",
		"dnode",
		"DTYPE",
		"EEDATA",
		"eeprom",
		"emio",
		"erpc",
		"ERRACKMASK",
		"ESCREG",
		"etohs",
		"fmmu",
		"halfbridge",
		"IBUS",
		"INCLAIM",
		"MBXBUFFERS",
		"MBXSIZE",
		"MBXSIZEBOOT",
		"MMIO",
		"mqtt",
		"objd",
		"OBJIDX",
		"odlist",
		"OUTREQ",
		"puback",
		"rsvd",
		"rxpdo",
		"safeoutput",
		"sdhc",
		"sdma",
		"smact",
		"SMENABLE",
		"STATEMASK",
		"SYNCTYPE",
		"txpdo",
		"UDINT",
		"VBUS",
		"zynq"
	],
	"cmake.options.statusBarVisibility": "visible",
	"cmake.options.advanced": {
		"kit": {
			"statusBarVisibility": "visible"
		},
		"build": {
			"statusBarVisibility": "visible"
		},
		"launch": {
			"statusBarVisibility": "visible"
		},
		"debug": {
			"statusBarVisibility": "visible"
		}
	},
